---
import BaseHead from '../../components/BaseHead.astro';
import Navbar from '../../components/Navbar.astro';
import Footer from '../../components/Footer.astro';
import CTA from '../../components/CTA.astro';
import { SITE_TITLE } from '../../consts';
import { getTrades, getTradeBySlug } from '../../utils/seo-data';

export async function getStaticPaths() {
	const trades = getTrades();
	return trades.map((trade) => ({
		params: { trade: trade.slug },
		props: { trade },
	}));
}

const { trade: tradeData } = Astro.props;
const tradeName = tradeData.plural;
const tradeDescription = tradeData.description;
const pageTitle = `${tradeData.title} | ${SITE_TITLE}`;
const pageDescription = tradeData.description;

const siteUrl = Astro.site || 'https://example.com';
const currentUrl = `${siteUrl}/trades/${tradeData.slug}`;
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={pageTitle} description={pageDescription} />
		<meta name="keywords" content={`${tradeName.toLowerCase()}, website builders, web design, ${tradeData.keywords.join(', ')}`} />
		<script type="application/ld+json" set:html={JSON.stringify({
			"@context": "https://schema.org",
			"@type": "LocalBusiness",
			"name": tradeData.title,
			"description": tradeData.description,
			"url": currentUrl,
			"telephone": "+447700171266",
			"priceRange": "££",
			"serviceType": `Website Design for ${tradeName}`,
			"areaServed": {
				"@type": "Country",
				"name": "United Kingdom"
			},
			"hasOfferCatalog": {
				"@type": "OfferCatalog",
				"name": "Website Design Services",
				"itemListElement": [
					{
						"@type": "Offer",
						"itemOffered": {
							"@type": "Service",
							"name": `Website Design for ${tradeName}`,
							"description": `Professional website design services for ${tradeName.toLowerCase()}`
						}
					}
				]
			}
		})} />
		<script type="application/ld+json" set:html={JSON.stringify({
			"@context": "https://schema.org",
			"@type": "Service",
			"serviceType": "Website Design",
			"provider": {
				"@type": "LocalBusiness",
				"name": "The Trade Site Builders"
			},
			"areaServed": {
				"@type": "Country",
				"name": "United Kingdom"
			},
			"audience": {
				"@type": "Audience",
				"audienceType": tradeName
			}
		})} />
		<script type="application/ld+json" set:html={JSON.stringify({
			"@context": "https://schema.org",
			"@type": "FAQPage",
			"mainEntity": [
				{
					"@type": "Question",
					"name": `How much does a website cost for ${tradeName.toLowerCase()}?`,
					"acceptedAnswer": {
						"@type": "Answer",
						"text": `Professional website design for ${tradeName.toLowerCase()} typically starts from £499. We offer packages designed specifically for tradespeople that include mobile-responsive design, local SEO optimisation, and phone call tracking to help you get more hot leads.`
					}
				},
				{
					"@type": "Question",
					"name": `How long does it take to build a website for ${tradeName.toLowerCase()}?`,
					"acceptedAnswer": {
						"@type": "Answer",
						"text": `Most ${tradeName.toLowerCase()} websites are completed within 2-3 weeks. This includes design, development, content creation, and SEO setup. We work efficiently to get you online and generating leads as quickly as possible.`
					}
				},
				{
					"@type": "Question",
					"name": `Will my website show up on Google?`,
					"acceptedAnswer": {
						"@type": "Answer",
						"text": `Yes, we optimise every website for local search. Your site will be optimised for searches like "${tradeName.toLowerCase()} near me" and "${tradeName.toLowerCase()} in [your city]". We focus on getting you hot lead phone calls from local customers.`
					}
				},
				{
					"@type": "Question",
					"name": `Do I need technical knowledge to manage my website?`,
					"acceptedAnswer": {
						"@type": "Answer",
						"text": `No technical knowledge required. We build your website so it's easy to update yourself, or we can handle updates for you. Our focus is on getting you phone calls, not making you manage complex systems.`
					}
				}
			]
		})} />
		<script type="application/ld+json" set:html={JSON.stringify({
			"@context": "https://schema.org",
			"@type": "BreadcrumbList",
			"itemListElement": [
				{
					"@type": "ListItem",
					"position": 1,
					"name": "Home",
					"item": siteUrl
				},
				{
					"@type": "ListItem",
					"position": 2,
					"name": tradeData.title,
					"item": currentUrl
				}
			]
		})} />
