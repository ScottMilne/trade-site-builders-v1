		<script>
		function waitForGSAP(callback) {
			if (typeof window !== 'undefined' && window.gsap && window.ScrollTrigger) {
				callback();
			} else {
				// Trigger GSAP loading if not already loaded
				if (typeof window.loadGSAP === 'function') {
					window.loadGSAP().then(() => {
						if (window.gsap && window.ScrollTrigger) {
							callback();
						} else {
							setTimeout(() => waitForGSAP(callback), 50);
						}
					});
				} else {
					setTimeout(() => waitForGSAP(callback), 50);
				}
			}
		}

			function initAnimations() {
				gsap.registerPlugin(ScrollTrigger);
				
				gsap.set('section > div > div > div', { opacity: 0, y: 30 });
				
				gsap.to('section > div > div > div', {
					scrollTrigger: {
						trigger: 'section > div > div > div',
						start: 'top 80%',
						toggleActions: 'play none none none'
					},
					opacity: 1,
					y: 0,
					duration: 0.8,
					stagger: 0.1,
					ease: 'power3.out'
				});
			}

			if (document.readyState === 'loading') {
				document.addEventListener('DOMContentLoaded', () => {
					waitForGSAP(initAnimations);
				});
			} else {
				waitForGSAP(initAnimations);
			}
		</script>
